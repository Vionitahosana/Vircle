<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="screenLiveOneVM" type="com.application.app.modules.screenliveone.data.viewmodel.ScreenLiveOneVM"/>
    </data>
    <ScrollView android:clipToPadding="false"
        android:fillViewport="true"
        android:id="@+id/scrollViewScrollview"
        android:layout_height="match_parent" android:layout_width="match_parent">
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumnfire"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="vertical">
            <ImageView android:id="@+id/imageFire"
                android:layout_height="@dimen/_23pxh"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_width="@dimen/_16pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_fire"
                tools:ignore="ContentDescription" tools:src="@drawable/img_fire"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnlinethree"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_13pxh"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_marginTop="@dimen/_4pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylebluegray_1">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumnellipseone"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_11pxh"
                    android:layout_marginStart="@dimen/_11pxh"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <LinearLayout android:id="@+id/linearRowten"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <LinearLayout
                            android:id="@+id/linearRowellipseone"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.91"
                            android:layout_width="0dp" android:orientation="horizontal">
                            <ImageView android:id="@+id/imageEllipseOne"
                                android:layout_height="@dimen/_35pxh"
                                android:layout_width="@dimen/_35pxh"
                                android:scaleType="centerCrop"
                                circular="@{true}"
                                imageRes="@{@drawable/img_ellipse1_3}"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse1_3"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtLeo"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_7pxv"
                                android:layout_marginStart="@dimen/_14pxh"
                                android:layout_marginTop="@dimen/_7pxv"
                                android:layout_weight="0.32"
                                android:layout_width="0dp"
                                android:singleLine="true"
                                android:text="@{screenLiveOneVM.screenLiveOneModel.txtLeo}"
                                style="@style/txtInterregular20" tools:text="@string/lbl_leo"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtGroupNine"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_8pxv"
                                android:layout_marginStart="@dimen/_17pxh"
                                android:layout_marginTop="@dimen/_8pxv"
                                android:layout_weight="0.35"
                                android:layout_width="0dp"
                                android:paddingBottom="@dimen/_3pxv"
                                android:paddingEnd="@dimen/_2pxh"
                                android:paddingStart="@dimen/_2pxh"
                                android:paddingTop="@dimen/_1pxv"
                                android:singleLine="true"
                                android:text="@{screenLiveOneVM.screenLiveOneModel.txtGroupNine}"
                                style="@style/txtSolidRounded" tools:text="@string/lbl_host"/>
                        </LinearLayout>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtTen"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_10pxv"
                            android:layout_marginStart="@dimen/_141pxh"
                            android:layout_marginTop="@dimen/_12pxv"
                            android:layout_weight="0.09"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{screenLiveOneVM.screenLiveOneModel.txtTen}"
                            style="@style/txtInterregular12" tools:text="@string/lbl_10"/>
                    </LinearLayout>
                    <TextView android:gravity="center_horizontal"
                        android:id="@+id/txtLanguage"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_1pxh"
                        android:layout_marginTop="@dimen/_9pxv"
                        android:layout_width="match_parent"
                        android:text="@{screenLiveOneVM.screenLiveOneModel.txtLanguage}"
                        style="@style/txtHappyMonkeyregular16_1" tools:text="@string/msg_live_bareng_yuk"/>
                </LinearLayout>
                <View android:background="@color/black_901"
                    android:id="@+id/lineLineThree"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_16pxv" android:layout_width="@dimen/_328pxh"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumnthread"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_8pxv"
                    android:layout_marginEnd="@dimen/_11pxh"
                    android:layout_marginStart="@dimen/_11pxh"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtThread"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{screenLiveOneVM.screenLiveOneModel.txtThread}"
                        style="@style/txtInterregular20" tools:text="@string/lbl_thread"/>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearRowellipseoneOne"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageEllipseOneOne"
                            android:layout_height="@dimen/_20pxh"
                            android:layout_width="@dimen/_20pxh"
                            android:scaleType="centerCrop"
                            circular="@{true}"
                            imageRes="@{@drawable/img_ellipse1_3}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse1_3"/>
                        <LinearLayout android:gravity="start"
                            android:id="@+id/linearColumnleoOne"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_7pxh"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_weight="0.75"
                            android:layout_width="0dp" android:orientation="vertical">
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtLeoOne"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_10pxh"
                                android:layout_width="wrap_content"
                                android:singleLine="true"
                                android:text="@{screenLiveOneVM.screenLiveOneModel.txtLeoOne}"
                                style="@style/txtInterregular12" tools:text="@string/lbl_leo"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txt12Juni2022"
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent"
                                android:singleLine="true"
                                android:text="@{screenLiveOneVM.screenLiveOneModel.txt12Juni2022}"
                                style="@style/txtInterregular10" tools:text="@string/lbl_12_juni_2022"/>
                        </LinearLayout>
                    </LinearLayout>
                    <TextView android:gravity="center_horizontal"
                        android:id="@+id/txtEchiroOdaKonf"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_width="match_parent"
                        android:text="@{screenLiveOneVM.screenLiveOneModel.txtEchiroOdaKonf}"
                        style="@style/txtHappyMonkeyregular16_1" tools:text="@string/msg_echiro_oda_konf"/>
                </LinearLayout>
            </LinearLayout>
            <View android:background="@color/gray_200"
                android:id="@+id/lineLineOne"
                android:layout_height="@dimen/_1pxv"
                android:layout_marginTop="@dimen/_324pxv" android:layout_width="@dimen/_355pxh"/>
            <LinearLayout android:id="@+id/linearRow"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_14pxh"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_marginTop="@dimen/_9pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:id="@+id/linearRowdownload"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_3pxv"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.5"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageDownload"
                        android:layout_height="@dimen/_25pxh"
                        android:layout_marginBottom="@dimen/_2pxv"
                        android:layout_width="@dimen/_25pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_download"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_download"/>
                    <ImageView android:id="@+id/imageUser"
                        android:layout_height="@dimen/_26pxh"
                        android:layout_marginStart="@dimen/_37pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="@dimen/_28pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_user"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_user"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearRowtrash"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_125pxh"
                    android:layout_weight="0.5"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageTrash"
                        android:layout_height="@dimen/_29pxh"
                        android:layout_marginBottom="@dimen/_4pxv"
                        android:layout_width="@dimen/_24pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_trash"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_trash"/>
                    <ImageView android:id="@+id/imageNotification"
                        android:layout_height="@dimen/_32pxh"
                        android:layout_marginStart="@dimen/_35pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="@dimen/_32pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_notification"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_notification"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</layout>

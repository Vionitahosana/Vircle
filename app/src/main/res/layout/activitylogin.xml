<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".MainActivity"
    android:background="@color/white_A700"

    >
    <FrameLayout android:id="@+id/frameLogin"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        style="@style/groupStylewhite_A700"
        android:background="@color/white_A700">

        <LinearLayout
            android:id="@+id/linearColumnlogovirclethree"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="top|center"
            android:layout_marginStart="@dimen/_30pxh"
            android:layout_marginEnd="@dimen/_30pxh"
            android:layout_marginBottom="@dimen/_10pxv"
            android:orientation="vertical">

            <FrameLayout
                android:id="@+id/frameStacklogovirclethree"
                android:layout_width="@dimen/_290pxh"
                android:layout_height="189dp"
                android:layout_marginTop="@dimen/_15pxh"
                android:layout_gravity="center_horizontal"
                android:layout_marginStart="@dimen/_4pxh">

                <ImageView
                    android:id="@+id/imageView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:srcCompat="@drawable/img_logovircle3" />
            </FrameLayout>


            <com.google.android.material.textfield.TextInputLayout
                android:layout_width="@dimen/_290pxh"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_1pxh"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                android:id="@+id/textInputLayout1"
                android:layout_gravity="center_horizontal">

                <com.google.android.material.textfield.TextInputEditText
                    android:id="@+id/inputEmail"
                    style="@style/etSolidRoundedOutline"
                    android:layout_width="@dimen/_290pxh"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|center"

                    android:background="@drawable/rectangle_bg_white_a700_border_black_900_radius_6"
                    android:drawableStart="@drawable/img_vector"
                    android:drawablePadding="@dimen/_19pxh"
                    android:ellipsize="end"
                    android:hint="@string/lbl_email"
                    android:inputType="textEmailAddress"
                    android:minHeight="48dp"
                    android:paddingStart="@dimen/_12pxh"
                    android:paddingTop="@dimen/_6pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingBottom="@dimen/_13pxv"
                    android:singleLine="true"
                    android:textColorHint="@color/gray_701"

                    tools:ignore="Autofill" />
            </com.google.android.material.textfield.TextInputLayout>

            <com.google.android.material.textfield.TextInputLayout
                android:layout_width="@dimen/_290pxh"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_15pxh"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                android:id="@+id/textInputLayout2"
                android:layout_gravity="center_horizontal">

                <com.google.android.material.textfield.TextInputEditText
                    android:id="@+id/inputPassword"
                    style="@style/etSolidRoundedOutline"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/rectangle_bg_white_a700_border_black_900_radius_6"
                    android:drawableStart="@drawable/img_lock"
                    android:drawableEnd="@drawable/img_airplane"
                    android:drawablePadding="@dimen/_19pxh"
                    android:ellipsize="end"
                    android:hint="@string/lbl_password"
                    android:inputType="textPassword"
                    android:minHeight="48dp"
                    android:paddingStart="@dimen/_13pxh"
                    android:paddingTop="@dimen/_8pxv"
                    android:paddingEnd="@dimen/_14pxh"
                    android:paddingBottom="@dimen/_11pxv"
                    android:singleLine="true"
                    android:textColorHint="@color/gray_701"
                    tools:ignore="Autofill" />
            </com.google.android.material.textfield.TextInputLayout>

            <TextView
                android:id="@+id/txtLupaPasword"
                style="@style/txtHappyMonkeyregular16_1"
                android:layout_width="@dimen/_130pxh"
                android:layout_height="wrap_content"
                android:layout_gravity="end"
                android:layout_marginStart="@dimen/_150pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_marginEnd="@dimen/_5pxh"
                android:ellipsize="end"
                android:singleLine="true"
                android:text="@string/lbl_lupa_pasword"
                tools:text="@string/lbl_lupa_pasword" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btnMasuk"
                style="@style/btnSolidRounded"
                android:layout_width="@dimen/_280pxh"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginStart="@dimen/_19pxh"
                android:layout_marginTop="@dimen/_25pxv"
                android:layout_marginEnd="@dimen/_15pxh"
                android:ellipsize="end"
                android:gravity="center"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_11pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingBottom="@dimen/_11pxv"
                android:singleLine="true"
                android:text="@string/lbl_masuk"
                android:textAllCaps="false"
                tools:text="@string/lbl_masuk" />

            <TextView
                android:id="@+id/txtAtau"
                style="@style/txtHappyMonkeyregular15"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_marginStart="@dimen/_19pxh"
                android:layout_marginTop="@dimen/_11pxv"
                android:layout_marginEnd="@dimen/_19pxh"
                android:ellipsize="end"
                android:singleLine="true"
                android:text="@string/lbl_atau"
                tools:text="@string/lbl_atau" />

            <FrameLayout
                android:id="@+id/frameStackgoogle"
                android:layout_width="@dimen/_280pxh"
                android:layout_height="@dimen/_41pxv"
                android:layout_gravity="center_horizontal"
                android:layout_marginStart="@dimen/_19pxh"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_marginEnd="@dimen/_15pxh">

                <LinearLayout
                    android:id="@+id/linearRowgoogle"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="top|center"
                    android:layout_marginStart="@dimen/_18pxh"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_marginEnd="@dimen/_18pxh"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/imageGoogle"
                        android:layout_width="@dimen/_25pxh"
                        android:layout_height="@dimen/_25pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_google"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_google" />

                    <TextView
                        android:id="@+id/txtMasukdenganGo"
                        style="@style/txtHappyMonkeyregular16_1"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_17pxh"
                        android:layout_marginTop="@dimen/_5pxv"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_weight="0.88"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@string/msg_masuk_dengan_go"
                        tools:text="@string/msg_masuk_dengan_go" />
                </LinearLayout>

                <View
                    android:id="@+id/viewRectangleNineteen"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_41pxv"
                    android:layout_gravity="start"
                    android:background="@drawable/rectangle_border_black_900_radius_20_5" />
            </FrameLayout>

            <LinearLayout
                android:id="@+id/linearRowfacebook"
                style="@style/groupStylecornerRadius"
                android:layout_width="@dimen/_280pxh"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_marginStart="@dimen/_19pxh"
                android:layout_marginTop="@dimen/_12pxv"
                android:layout_marginEnd="@dimen/_15pxh"
                android:gravity="start"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageFacebook"
                    android:layout_width="@dimen/_25pxh"
                    android:layout_height="@dimen/_25pxh"
                    android:layout_marginStart="@dimen/_18pxh"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:layout_marginBottom="@dimen/_8pxv"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_facebook"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_facebook" />

                <TextView
                    android:id="@+id/txtMasukdenganFa"
                    style="@style/txtHappyMonkeyregular16_1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_7pxh"
                    android:layout_marginTop="@dimen/_11pxv"
                    android:layout_marginEnd="@dimen/_6pxh"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_weight="0.89"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@string/msg_masuk_dengan_fa"
                    tools:text="@string/msg_masuk_dengan_fa" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearRowlanguage"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_40pxh"
                android:layout_marginTop="@dimen/_30pxv"
                android:layout_marginEnd="@dimen/_19pxh"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/txtLanguage"
                    style="@style/txtHappyMonkeyregular16_1"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginStart="@dimen/_26pxh"
                    android:layout_weight="0.75"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@string/msg_belum_punya_aku"
                    tools:text="@string/msg_belum_punya_aku" />

                <TextView
                    android:id="@+id/txtDaftar"
                    style="@style/txtHappyMonkeyregular16_1"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginEnd="@dimen/_40pxh"
                    android:layout_marginBottom="0dp"
                    android:layout_weight="0.25"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:textColor="@color/light_blue_400"
                    android:text="@string/lbl_daftar"
                    tools:text="@string/lbl_daftar" />
            </LinearLayout>

            <TextView
                android:id="@+id/txtDenganmelanjut"
                style="@style/txtIcelandregular16"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_35pxv"
                android:layout_marginEnd="@dimen/_1pxh"
                android:gravity="center_horizontal"
                android:text="@string/msg_dengan_melanjut"
                tools:text="@string/msg_dengan_melanjut" />
        </LinearLayout>
    </FrameLayout>
</androidx.constraintlayout.widget.ConstraintLayout>